{
    "name": "TaLK",
    "version": "0.0.2",
    "description": "Record and tag audio in meetings.",
    "long_description": "TaLK is used to gather and record knowledge from people. Setup a meeting agenda, attendees, and then have an easy way to record and tag audio while in a meeting. ",
    "url": "https://github.com/ryanramage/TaLK",
    "categories": ["audio", "meetings"],
    "attachments": ["css", "img", "jam", "js",  "index.html", "application.appcache", "templates", "static"],
    "legacy_mode" : true,
    "install_with_no_reader" : true,
    "icons": {
        "16": "img/promo/icon_16.png",
        "48": "img/promo/icon_48.png",
        "96": "img/promo/icon_96.png",
        "128": "img/promo/icon_128.png"
    },
    "promo_images": {
        "small": "img/promo/promo_small.png"
    },
    "screenshots": [
        "img/promo/screenshot1.png",
        "img/promo/screenshot2.png",
        "img/promo/screenshot3.png"
    ],

    "modules": "lib",
    "load": "lib/app",
    "less": {
        "compress": true,
        "compile": ["css/main.less"],
        "remove_from_attachments": true
    },
    "cache_manifests": [
             "application.appcache"
         ],
    "handlebars": {
        "all_partials": true,
        "templates": "templates"
    },
    "jam" : {
        "include" : ["js/app", "domReady"]
    },
    "dependencies": {
        "modules": null,
        "attachments": null,
        "properties": null,
        "underscore": null,
        "less-precompiler": "1.3.0-kanso.7",
        "cache-manifest-timestamp": null,
        "git-info" : null,
        "kanso-jam" : null,
        "couch-audio-recorder" : null
    }
}
