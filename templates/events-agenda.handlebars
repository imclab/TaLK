<div id="{{_id}}" data-id="{{_id}}" class="agenda-listing">

    <h2>{{name}}</h2>




    <table class="table striped-table " {{#unless items}}style="display: none;"{{/unless}}>
      <thead>
        <tr>
          <th>Type</th>
          <th>Colour</th>
          <th>Text</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
    {{#each items}}
    <tr data-id="{{id}}" id="{{id}}">
      <td>{{type}}</td>
      <td><input class="simple_color" value="#{{colour}}" data-id={{id}} /> </td>
      <td>{{text}}</td>
      <td><button class="btn delete"  data-id={{id}} >x</button></td>
    </tr>
    {{/each}}
      </tbody>
    </table>

    <div class="row">
        <div class="span3">
            <div class="topicAutoComplete">
                <input type="hidden" name="name" placeholder="Add Topic..." class="input-xxlarge"/>
            </div>
        </div>
    </div>



</div>